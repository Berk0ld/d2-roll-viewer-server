{
  "name": "d2-roll-viewer-web",
  "version": "0.0.1",
  "description": "D2 Roll viewer web",
  "author": "Alexey Demedetskii, Andrey Moskvin",
  "main": "src/index.tsx",
  "scripts": {
    "lint": "tslint -p ./",
    "tsc": "tsc -p ./ --noEmit",
    "tsc:watch": "tsc -p ./ --noEmit -w",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "watch-debug": "yarn install && tsc-watch --project ./src --onSuccess \"npm run debug\"",
    "debug": "node --inspect=0.0.0.0:9223 ./build/index.js"
  },
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  }
}
